     var defaultMenu = "代码生成";        var menus = {            "模板配置": "template/template.jsp",            "用户管理": "user/user.jsp",            "连接管理": "connection/connection.jsp"        };        menus[defaultMenu] = "/page/codebuilder/codebuilder.jsp";        var tabs = new cmp.tabs("#tabPanel");        $("#northbtns").find("a.north-btn").click(function () {            var el = $(this),                menuKey = el.text();            tabs.openTabs({                title: menuKey,                url: menus[menuKey]            });        });        tabs.openTabs({title: defaultMenu,closable:false, url: menus[defaultMenu]});        /*var iframe=$("iframe")[0];        song.loaded(iframe,function () {            var editor=iframe.contentWindow.editor;            editor.loadFile(document.baseURI,"html");        })*/    });</script><div data-options="region:'north'" class="north" id="northbtns">    <a class="north-btn">代码生成</a>    <span class="north-line">|</span>    <a class="north-btn">模板配置</a>    <span class="north-line">|</span>    <a class="north-btn" data-url="">用户管理</a>    <span class="north-line">|</span>    <a class="north-btn" data-url="">连接管理</a>    <span class="north-logo">开发者平台</span></div><div data-options="region:'center'" class="center">    <div id="tabPanel">    </div></div></body></html><style type="text/css">        .north {        background: #017fe1;        height: 42px;        border: none 0px;        zoom: 1    }    .north a,.north span {        height: 42px;        line-height:42px;    }    .center {        border: none 0px;    }    .north-btn {        font-size: 14px;        color: #ffffff;        text-align: center;        display: block;        float: right;        width: 100px;        line-height: 42px;        cursor: pointer;    }    .north-btn:hover {        background: #2566d1    }    .north-line {        display: block;        border-right: 1px solid #ffffff;        float: right;        overflow: hidden;        border-left: 1px solid #444444;        font-size: 0px;        width: 0px;        opacity: 0.4;        filter: Alpha(Opacity=40);    }    .north-logo {        color:#ffffff; font-size: 20px; font-weight: bold; font-family: 幼圆;float: left; display: block;        margin-left:20px;    }</style>