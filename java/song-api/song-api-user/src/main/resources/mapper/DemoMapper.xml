<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="song.api.user.dao.IDemoDao">    <sql id="select">        SELECT * FROM demo    </sql>    <select id="getByName" parameterType="string" resultType="song.api.user.model.Demo">        <include refid="select"/>        <where>            <if test="name!=null and name!=''">                name=#{name}            </if>        </where>    </select></mapper>