<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.studio.api.dao.IColumnDao">    <sql id="selectColumn">        SELECT * FROM studio_column    </sql>    <select id="getByTableId" parameterType="String" resultType="com.studio.model.Column">        <include refid="selectColumn"/>        <where>            <if test="tableId!=null and tableId!=''">                tableId=#{tableId}            </if>        </where>    </select></mapper>